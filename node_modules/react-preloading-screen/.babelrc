{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"chrome": 55
				},
				"modules": false,
				"forceAllTransforms": true
			}
		],
		"@babel/preset-flow",
		"@babel/preset-react"
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-proposal-object-rest-spread"
	]
}
